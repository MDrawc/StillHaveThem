var view = '<%= view %>';

$("#show-more").html("<%= j(render :partial => 'results', :locals => { view: view }) %>");
$("#show-more").attr('id', 'showed_more');

switch (view) {
    case 'list_view':
        break;
    case 'cover_view':
        cleanOffcanvas()
        fitNameInNoCover();
        morePlatforms();
        break;
    case 'panel_view':
        moreInfo();
        $('.g-drop').slideUp(0);
        break;
}
