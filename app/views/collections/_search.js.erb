var ids = [];
var arr = $('.g-drop');
arr.each(function() {
    if ($(this).attr('style') == 'display: block;') {
        ids.push($(this).attr('id'));
    }
});

cleanOffcanvas();

$(".my-games").html('<%= j(render partial: "my_games/#{ @view }", locals: { collection: @collection }) %>');
$('.game_search #view').attr('value', '<%= @view %>')

$('#r-q').text(<%= raw query %>);
$('#r-s').text('<%= sort %>');

ids.forEach(id => $('#' + id).show());

flip_offc(<%= user_id %>);
moreInfo();
fitNameInNoCover();
