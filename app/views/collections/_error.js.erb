$("#coll-icon-input").replaceWith("<%= j(render partial: 'warning', locals: { error: @error }) %>");

$('#collection_name').on('input', function() {
    var $icon = $('#coll-icon-input');

    if ($icon.hasClass('warning-icon')) {
        $icon.replaceWith("<span id='coll-icon-input'><%= j(svg 'label') %></span>");
    }

});
